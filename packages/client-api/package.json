{
  "name": "@lib/client-api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean-dist && tsc",
    "clean-dist": "rimraf dist/*",
    "prettier:check": "prettier --check \"**/*.ts*\"",
    "prettier:fix": "prettier --write \"**/*.ts*\"",
    "lint": "eslint \"**/*.ts*\"",
    "lint:fix": "eslint --fix \"**/*.ts*\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint"
    ]
  },
  "prettier": "@config/prettier-config",
  "dependencies": {
    "@lib/client-config": "*",
    "@lib/types": "*",
    "axios": "^0.27.2",
    "nanoid": "^3.3.4"
  },
  "devDependencies": {
    "@config/prettier-config": "*",
    "lint-staged": "^13.0.3",
    "typescript": "~4.8.4"
  }
}
