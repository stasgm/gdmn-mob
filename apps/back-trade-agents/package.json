    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@config/eslint-config": "*",
    "@config/prettier-config": "*",
    "@types/bcrypt": "^5.0.0",
    "@types/nedb": "^1.8.12",
    "eslint": "^7.29.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.13",
    "pm2": "^5.1.0",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0"
