{
  "expo": {
    "name": "gdmn-mobile",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "svg"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gdmn.app",
      "permissions": [
        "LOCATION",
        "CALENDAR"
      ],
      "versionCode": 100
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
